{
  "build": ["linux", "armv7", "macos-amd64", "macos-arm64"],
  "include": [
    {
      "build": "linux",
      "os": "ubuntu-latest",
      "target": "x86_64-unknown-linux-gnu"
    },
    {
      "build": "armv7",
      "os": "ubuntu-latest",
      "target": "armv7-unknown-linux-gnueabihf",
      "is_arm": true
    },
    {
      "build": "macos-amd64",
      "os": "macos-latest",
      "target": "x86_64-apple-darwin",
      "is_macos": true,
      "zip_archive": true
    },
    {
      "build": "macos-arm64",
      "os": "macos-latest",
      "target": "aarch64-apple-darwin",
      "is_macos": true,
      "zip_archive": true
    }
  ]
}
